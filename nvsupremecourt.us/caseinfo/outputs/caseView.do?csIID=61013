<script type="text/javascript">
function toggleTable(elem)
{
	var $td = $(elem);
	
	$td.parents('tr.TableHeading').siblings('tr').each(function() {
		if ($(this).is(":visible")) {
			$(this).hide();
		} else {
			$(this).show();
		}
	});

	var text = $td.text();
	var firstChar = text.charAt(0);
	if (firstChar == '+')
		firstChar = '-';
	else
		firstChar = '+';

	$td.text(firstChar + text.substring(1, text.length));
}
</script>

	
	
	
