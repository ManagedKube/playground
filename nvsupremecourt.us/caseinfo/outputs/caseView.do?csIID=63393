	$("#application-table").hide();
	var ReCAPTCHACallback = new function() {
		var PUBLIC_KEY = '6LfLaPYjAAAAAOFDkTzc1xtte8VpSDsnuOBeIfIK';

		this.request = function(action, onSuccess, onError) {
			if (window.grecaptcha) {
